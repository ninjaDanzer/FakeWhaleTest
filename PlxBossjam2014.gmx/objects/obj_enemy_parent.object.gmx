            <string>///Enemy behaviour code
//STATES
// 0 = Idle
// 1 = Suspicious
// 2 = Surprised
// 3 = Sound alarm
// 4 = Attack player
//TYPES
// 0 = stands still
// 1 = patrols
//MOVEDIR
// -1.0 = left
// 0 = stand still
// 1.0 = right

//Guard or patrol
if(state == 0) {
    if(type == 0) {
        if( (xstart+10) &lt; x) {
            movedir = -1.0;
        }
        else if( (xstart-10) &gt; x){
            movedir = 1.0;
        }
    }
    else if(type == 1) {
        if( collision_rectangle(x,y, x+64,y, obj_tile_mockup, true, true) ) {
            movedir = -1.0;
            image_xscale = -1;
        }
        else if( collision_rectangle(x,y, x-64,y, obj_tile_mockup, true, true) ) {
            movedir = 1.0;
            image_xscale = 1;
        }
    }
    speed_modifier = 1.5;
    interacts = 0;
}

//Move towards player carefully, try to open doors.
if(state == 1) {
    if( obj_player.x &lt; x) {
        movedir = -1.0;
    }
    else {
        movedir = 1.0;
    }
    speed_modifier = 1.0;
    interacts = 1;
}

//The enemy is surprised, and soon goes into attack mode
if(state == 2) {
    if( (alarm[0] &lt;= 0 )) {
        alarm[0] = room_speed * 2;
    }
    speed_modifier = 0;
    interacts = 1;
}

if(state == 3) {
    nearest_alarm = instance_nearest(x,y,obj_alarm);
    if( (nearest_alarm.y &gt; y+224) || (nearest_alarm.y &lt; y-224) ) {
        state = 4;
    }
    else {
        if( nearest_alarm.x &lt; x) {
            movedir = -1.0;
        }
        else {
            movedir = 1.0;
        }
    }
    speed_modifier = 1.25;
    interacts = 1;
}

if(state == 4) {
    if( obj_player.x &lt; x) {
        movedir = -1.0;
    }
    else {
        movedir = 1.0;
    }
    speed_modifier = 1.5;
    interacts = 1;
}

x += 1 * movedir * speed_modifier;
</string>
