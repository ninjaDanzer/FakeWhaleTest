            <string>//Check if the player crouches and modify movement speed
var speed_multiplier;
if(input_check(BUTTON_CROUCH)) {
    speed_multiplier = 90 / room_speed;
}
else {
    speed_multiplier = 180 / room_speed;;
}

//flip sprite
if( (input_movement_x() &lt; 0) ) {
image_xscale = -1;
}
else if(input_movement_x() &gt; 0)  {
image_xscale = 1;
}

//move player
x += input_movement_x() * speed_multiplier;

//fade player
if(image_alpha&gt;0 &amp;&amp; PlayerHidden==true){
        image_alpha -= 0.01;
}
if(image_alpha&lt;1 &amp;&amp; PlayerHidden==false){
        image_alpha += 0.01;
}

//Switch stationary and moving + walking sound
if(input_movement_x()==0){
    image_speed=0;
    //audio_pause_sound(snd_whale_walking_first);
}
else{ 
    //audio_resume_sound(snd_whale_walking_first);
    image_speed=0.1;
}

// Temporary nosie test code
if (input_check_pressed(BUTTON_ACTION)) {
    make_noise(x, y, 300);
}
</string>
